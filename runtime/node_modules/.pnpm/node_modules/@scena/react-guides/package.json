{
    "name": "@scena/react-guides",
    "version": "0.28.2",
    "description": "A React Guides component that can draw ruler and manage guidelines.",
    "main": "./dist/guides.cjs.js",
    "module": "./dist/guides.esm.js",
    "sideEffects": false,
    "types": "declaration/index.d.ts",
    "scripts": {
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "start": "rm -rf ./node_modules/.cache && SKIP_PREFLIGHT_CHECK=true react-scripts start",
        "build": "rollup -c && npm run declaration && print-sizes ./dist ",
        "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json"
    },
    "keywords": [
        "scene",
        "scenejs",
        "scene.js",
        "scena",
        "guides",
        "guidelines",
        "scroller",
        "ruler",
        "react"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/daybrush/guides/blob/master/packages/react-guides"
    },
    "author": "Daybrush",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/daybrush/guides/issues"
    },
    "homepage": "https://daybrush.com/guides",
    "files": [
        "./*",
        "src/*",
        "dist/*",
        "declaration/*",
        "README.md"
    ],
    "devDependencies": {
        "@daybrush/builder": "^0.1.0",
        "@scenejs/effects": "^1.0.1",
        "@types/react": "^16.8.18",
        "@types/react-color": "^3.0.1",
        "@types/react-dom": "^16.8.4",
        "print-sizes": "^0.2.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-infinite-viewer": "^0.20.0",
        "react-scripts": "^3.0.1",
        "shape-svg": "^0.3.3",
        "tslint": "^5.20.1",
        "typescript": "^4.5.0 <4.6.0"
    },
    "dependencies": {
        "@daybrush/utils": "^1.12.1",
        "@scena/dragscroll": "^1.3.0",
        "@scena/react-ruler": "^0.19.0",
        "css-to-mat": "^1.0.3",
        "framework-utils": "^1.1.0",
        "gesto": "^1.18.1",
        "react-css-styled": "^1.1.8"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
