#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/code/magic-editor/runtime/node_modules/.pnpm/registry.npmmirror.com+@tmagic+cli@1.3.0-beta.2/node_modules/@tmagic/cli/bin/node_modules:/mnt/d/code/magic-editor/runtime/node_modules/.pnpm/registry.npmmirror.com+@tmagic+cli@1.3.0-beta.2/node_modules/@tmagic/cli/node_modules:/mnt/d/code/magic-editor/runtime/node_modules/.pnpm/registry.npmmirror.com+@tmagic+cli@1.3.0-beta.2/node_modules/@tmagic/node_modules:/mnt/d/code/magic-editor/runtime/node_modules/.pnpm/registry.npmmirror.com+@tmagic+cli@1.3.0-beta.2/node_modules:/mnt/d/code/magic-editor/runtime/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/code/magic-editor/runtime/node_modules/.pnpm/registry.npmmirror.com+@tmagic+cli@1.3.0-beta.2/node_modules/@tmagic/cli/bin/node_modules:/mnt/d/code/magic-editor/runtime/node_modules/.pnpm/registry.npmmirror.com+@tmagic+cli@1.3.0-beta.2/node_modules/@tmagic/cli/node_modules:/mnt/d/code/magic-editor/runtime/node_modules/.pnpm/registry.npmmirror.com+@tmagic+cli@1.3.0-beta.2/node_modules/@tmagic/node_modules:/mnt/d/code/magic-editor/runtime/node_modules/.pnpm/registry.npmmirror.com+@tmagic+cli@1.3.0-beta.2/node_modules:/mnt/d/code/magic-editor/runtime/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tmagic.js" "$@"
else
  exec node  "$basedir/../../bin/tmagic.js" "$@"
fi
